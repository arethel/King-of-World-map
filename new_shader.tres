[gd_resource type="Shader" format=2]

[resource]
code = "shader_type spatial;

uniform sampler2D textur;

void vertex(){
//	VERTEX.y+=sin(VERTEX.x*10.0*VERTEX.z+TIME*2.0);
	VERTEX.x*=10.0;
	VERTEX.z*=10.0;
}



void fragment(){
	vec3 color = texture(textur,UV).rgb;
	float darkness = (color.r+color.b+color.g)/3.0;
	if(darkness>0.1){
		ALBEDO=color;
	}
		
}

"
